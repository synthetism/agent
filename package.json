{
  "name": "@synet/agent",
  "version": "0.0.0",
  "description": "",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synthetism/agent.git"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rimraf dist",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "format": "biome format --write ./src ./test",
    "type-check": "tsc --noEmit --skipLibCheck",
    "prepublishOnly": "npm run clean && npm run lint && npm run type-check && npm run test && npm run build",
    "version:dev": "npm version --no-git-tag-version prerelease --preid=dev",
    "version:patch": "npm version --no-git-tag-version patch",
    "version:minor": "npm version --no-git-tag-version minor",
    "version:major": "npm version --no-git-tag-version major",
    "publish:dev": "npm publish --registry=http://registry.dig.run/ --tag dev",
    "publish:prod": "npm publish --registry=https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@synet/crypto": "^1.0.1",
    "@synet/email": "^1.0.2",
    "@synet/fs": "^2.0.1",
    "@synet/fs-ai": "^1.0.0",
    "@synet/hasher": "^1.0.1",
    "@synet/weather": "^1.0.0",
    "@types/node": "^24.1.0",
    "@vitest/coverage-v8": "^3.1.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "ai",
    "openai",
    "anthropic",
    "claude",
    "gemini",
    "deepseek",
    "mistral",
    "unit-architecture",
    "consciousness-transfer",
    "function-calling",
    "multi-provider",
    "zero-dependency",
    "production-ready",
    "synthetism"
  ],
  "author": "Synet Team",
  "homepage": "https://synthetism.ai",
  "email": "core@synthetism.ai",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@synet/ai": "^1.0.4",
    "@synet/unit": "^1.0.7"
  }
}
